{
    "multipart": [
        {
            "apply": {
                "model": "oceanmaze:block/maze_cell_in",
                "x": 270,
                "uvlock": true
            }
        },
        {
            "apply": {
                "model": "oceanmaze:block/maze_cell_in",
                "x": 90,
                "uvlock": true
            }
        },
        {
            "when": {
                "north": true
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_out"
            }
        },
        {
            "when": {
                "east": true
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_out",
                "y": 90,
                "uvlock": true
            }
        },
        {
            "when": {
                "south": true
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_out",
                "y": 180,
                "uvlock": true
            }
        },
        {
            "when": {
                "west": true
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_out",
                "y": 270,
                "uvlock": true
            }
        },
        {
            "when": {
                "north": false
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_in"
            }
        },
        {
            "when": {
                "east": false
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_in",
                "y": 90,
                "uvlock": false
            }
        },
        {
            "when": {
                "south": false
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_in",
                "y": 180,
                "uvlock": false
            }
        },
        {
            "when": {
                "west": false
            },
            "apply": {
                "model": "oceanmaze:block/maze_cell_in",
                "y": 270,
                "uvlock": false
            }
        }
    ]
}